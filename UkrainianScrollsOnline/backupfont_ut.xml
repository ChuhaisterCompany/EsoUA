<GuiXml>
    <!-- Optional backup font settings for Chinese language mode. -->
    <!-- These backup fonts will be used in place of the missing glyphs in the original font -->
    <BackupFont originalFont="$(MEDIUM_FONT)" backupFont="UkrainianScrollsOnline/fonts-ua/e-Ukraine-Medium.otf"/>
    <BackupFont originalFont="$(BOLD_FONT)" backupFont="UkrainianScrollsOnline/fonts-ua/e-Ukraine-Bold.otf"/>
    <BackupFont originalFont="$(CHAT_FONT)" backupFont="UkrainianScrollsOnline/fonts-ua/e-Ukraine-Light.otf"/>
    <BackupFont originalFont="$(GAMEPAD_BOLD_FONT)" backupFont="UkrainianScrollsOnline/fonts-ua/e-Ukraine-Bold.otf"/>
    <BackupFont originalFont="$(GAMEPAD_LIGHT_FONT)" backupFont="UkrainianScrollsOnline/e-Ukraine-Light.otf"/>
    <BackupFont originalFont="$(GAMEPAD_MEDIUM_FONT)" backupFont="UkrainianScrollsOnline/fonts-ua/e-Ukraine-Medium.otf"/>

</GuiXml>
